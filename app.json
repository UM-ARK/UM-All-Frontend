{
  "expo": {
    "name": "UMALL",
    "displayName": "UMALL",
    "slug": "umall",
    "version": "26.1.1",
    "icon": "./src/static/img/logo.png",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "icon": "./src/static/img/logo.png",
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "one.umall",
      "buildNumber": "26.1.1",
      "deploymentTarget": "15.1"
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "googleServicesFile": "./google-services.json",
      "package": "one.umall",
      "versionCode": 74,
      "adaptiveIcon": {
        "foregroundImage": "./src/static/img/logo.png",
        "backgroundColor": "#E6AB52"
      }
    },
    "plugins": [
      "expo-web-browser",
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ]
  }
}